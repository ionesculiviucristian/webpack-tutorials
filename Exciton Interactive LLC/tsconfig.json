{
    "compileOnSave": false,
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "noImplicitAny": true,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "wwwroot/js",
        "lib": [ "es2017", "dom" ],
        "jsx": "react"
    },
    "exclude": [
        "node_modules",
        "wwwroot"
    ]
}